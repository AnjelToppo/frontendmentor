{
  "name": "password-generator-app",
  "private": true,
  "version": "0.0.0",
  "description": "This is a password generator app build with react and sass",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "watchsass": "sass --watch src/index.sass src/index.css",
    "compilesass": "sass src/index.sass src/index.css",
    "site": "npm run build && npm run compilesass"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.1"
  },
  "author": "Anjel Toppo",
  "license": "ISC"
}
